{
    "name": "wirgen/laravel-keitaro",
    "type": "library",
    "description": "Laravel wrapper for Keitaro Admin API",
    "keywords": [
        "laravel",
        "keitaro",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Nikiforov",
            "email": "my.wirgen@gmail.com",
            "homepage": "https://wirgen.by"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "wirgen/keitaro": "^1.0",
        "illuminate/support": "^5.7|^6|^7|^8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12.98",
        "vimeo/psalm": "^4.10",
        "orchestra/testbench": "^6.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wirgen\\Larataro\\KeitaroServiceProvider"
            ],
            "aliases": {
                "Keitaro": "Wirgen\\Larataro\\Keitaro"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Wirgen\\Larataro\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
